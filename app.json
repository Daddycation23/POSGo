{
  "expo": {
    "name": "Walking_POS",
    "slug": "Walking_POS",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/POSGo-Icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/POSGo-Icon.png",
      "resizeMode": "native",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/POSGo-Icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.daddycation.POSGo"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/POSGo-Icon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "The app accesses your camera to let you take photos for menu items."
        }
      ],
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "8ada2d07-0635-4164-983e-288f864fe2e1"
      }
    }
  }
}
